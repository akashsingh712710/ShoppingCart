@model Entities.Product

<h1>Create Product</h1>

<script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-3.3.1.min.js"></script>
<script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.17.0/jquery.validate.min.js"></script>
<script src="https://ajax.aspnetcdn.com/ajax/jquery.validate.unobtrusive/3.2.10/jquery.validate.unobtrusive.min.js"></script>


<form asp-action="Create">
    <div class="form-group">
        <label asp-for="ProductCode"></label>
        <input asp-for="ProductCode" class="form-control" />
        <span asp-validation-for="ProductCode" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ProductName"></label>
        <input asp-for="ProductName" class="form-control" />
        <span asp-validation-for="ProductName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Manufacturer"></label>
        <input asp-for="Manufacturer" class="form-control" />
        <span asp-validation-for="Manufacturer" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ShippingNo"></label>
        <input asp-for="ShippingNo" class="form-control" />
        <span asp-validation-for="ShippingNo" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="SerialNo"></label>
        <input asp-for="SerialNo" class="form-control" />
        <span asp-validation-for="SerialNo" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="BatchNo"></label>
        <input asp-for="BatchNo" class="form-control" />
        <span asp-validation-for="BatchNo" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="MRP"></label>
        <input asp-for="MRP" class="form-control" />
        <span asp-validation-for="MRP" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Quantity"></label>
        <input asp-for="Quantity" class="form-control" />
        <span asp-validation-for="Quantity" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>
